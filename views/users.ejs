<!DOCTYPE html>
<html>
    <head>
        <% include ../views/partials/head.ejs %>
        <script src="/javascripts/makeActive.js"></script>
    </head>
    <body page-type="users">
    <% include ../views/partials/header.ejs %>
    <div class="container">
        <h1><%= title %></h1>
        <div class="panel panel-success">
            <div class="panel-heading">Players list</div>
            <table class="table">
                <tr>
                    <th>#</th>
                    <th>Name</th>
                    <th>Victories</th>
                    <th>Loses</th>
                </tr>
                <% for(var i=0; i<list.length; i++) { %>
                <tr>
                    <td> <%= i + 1 %> </td>
                    <td> <%= list[i][0] %> </td>
                    <td> <%= list[i][1] %> </td>
                    <td> <%= list[i][2] %> </td>
                </tr>
                <% } %>
            </table>
        </div>
    </div>
    </body>
</html>
